<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <netTcpBinding>
              <binding name="NetTcpBinding_IC2CService" closeTimeout="00:05:00"
                    openTimeout="00:05:00" receiveTimeout="00:10:00" sendTimeout="00:05:00"
                    transactionFlow="false" transferMode="Buffered" transactionProtocol="OleTransactions"
                    hostNameComparisonMode="StrongWildcard" listenBacklog="10"
                    maxBufferPoolSize="524288" maxBufferSize="65536" maxConnections="10"
                    maxReceivedMessageSize="65536">
                   <readerQuotas maxDepth="32" maxStringContentLength="8192" maxArrayLength="16384"
                      maxBytesPerRead="4096" maxNameTableCharCount="16384" />
                   <reliableSession ordered="true" inactivityTimeout="00:10:00"
                    enabled="false" />
                <security mode="None"></security>
              </binding>
            </netTcpBinding>
        </bindings>
        <client>
            <endpoint address="net.tcp://admin-pc/C2CService/Service.svc"
                binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IC2CService"
                contract="WebServices.IC2CService" name="NetTcpBinding_IC2CService">
                <identity>
                    <servicePrincipalName value="host/Admin-PC" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>